<div class="box">
    @Html.Partial("Forms/_BoxHeader", new BoxHeaderViewModel("Documentos", "fa fa-folder", true))
    <div class="box-content" data-collapsed="true">
        <table id="documentsGrid" title="Documentos" style="width:100%;height:250px"></table>
        <div id="documentsToolbar">
            <button type="button" data-buttontype="add" class="btn btn-primary btn-xs">
                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
                Novo
            </button>
            <button type="button" data-buttontype="edit" class="btn btn-info btn-xs" disabled="disabled">
                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                Editar
            </button>
            <button type="button" data-buttontype="delete" class="btn btn-danger btn-xs" disabled="disabled">
                <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                Remover
            </button>
        </div>
    </div>
</div>