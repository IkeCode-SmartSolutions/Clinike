<div class="box">
    @Html.Partial("Forms/_BoxHeader", new BoxHeaderViewModel("Documentos", "fa fa-folder", true))
    <div class="box-content" data-collapsed="true">
        <table id="documentsGrid" title="Documentos" style="width:850px;height:250px"></table>
        <div id="documentsToolbar">
            <a href="#" class="easyui-linkbutton" iconcls="icon-add" plain="true" onclick="documents.New()">Novo</a>
            <a href="#" class="easyui-linkbutton" iconcls="icon-edit" plain="true" onclick="documents.Edit()">Editar</a>
            <a href="#" class="easyui-linkbutton" iconcls="icon-remove" plain="true" onclick="documents.Delete()">Remover</a>
        </div>
    </div>
</div>