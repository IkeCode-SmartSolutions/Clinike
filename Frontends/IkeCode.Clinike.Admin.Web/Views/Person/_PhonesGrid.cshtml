<div class="box">
    @Html.Partial("Forms/_BoxHeader", new BoxHeaderViewModel("Telefones", "fa fa-phone", true))
    <div class="box-content" data-collapsed="true" style="display: none;">
        <table id="phonesGrid" class="table table-condensed table-hover table-striped">
            <thead>
                <tr>
                    <th data-column-id="Id" data-visible="false" data-identifier="true" data-type="numeric">ID</th>
                    <th data-column-id="Number" data-formatter="Number" data-type="numeric">Número</th>
                    <th data-column-id="PhoneType">Tipo</th>
                    <th data-column-id="commands" data-formatter="commands" data-sortable="false">Ações</th>
                </tr>
            </thead>
        </table>
    </div>
    
</div>