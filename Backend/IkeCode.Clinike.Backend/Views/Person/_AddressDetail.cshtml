@{
    Layout = null;
}
<div class="col-sm-12"S>
    <form class="form-horizontal" role="form" id="formAddressDetail">
        <input type="hidden" data-bind="value: Id" name="Id" />
        <input type="hidden" data-bind="value: DateIns" name="DateIns" />
        <input type="hidden" data-bind="value: LastUpdate" name="LastUpdate" />
        <input type="hidden" data-bind="value: PersonId" name="PersonId" />
        <div class="form-group">
            <label for="inputStreet" class="col-sm-3 control-label">Rua</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" data-bind="textInput: Street" name="Street" id="inputStreet" placeholder="Rua" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputNumber" class="col-sm-3 control-label">Número</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: Number" name="Number" id="inputNumber" placeholder="nº" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputComplement" class="col-sm-3 control-label">Complemento</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: Complement" name="Complement" id="inputComplement" placeholder="Complemento" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputNeighborhood" class="col-sm-3 control-label">Bairro</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: Neighborhood" name="Neighborhood" id="inputNeighborhood" placeholder="Bairro" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputCity" class="col-sm-3 control-label">Cidade</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: City" name="City" id="inputCity" placeholder="Cidade" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputState" class="col-sm-3 control-label">UF</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: State" name="State" id="inputState" placeholder="UF" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputCity" class="col-sm-3 control-label">CEP</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: ZipCode" name="ZipCode" id="inputZipCode" placeholder="00000-000" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputCity" class="col-sm-3 control-label">Tipo</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: AddressType" name="AddressType" id="inputAddressType" placeholder="Tipo" />
            </div>
        </div>
    </form>
</div>