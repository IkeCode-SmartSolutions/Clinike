@{
    Layout = null;
}
<div class="col-sm-12"S>
    <form class="form-horizontal" role="form" id="formDocumentDetail">
        <input type="hidden" data-bind="value: Id" name="Id" />
        <input type="hidden" data-bind="value: DateIns" name="DateIns" />
        <input type="hidden" data-bind="value: LastUpdate" name="LastUpdate" />
        <input type="hidden" data-bind="value: PersonId" name="PersonId" />
        <div class="form-group">
            <label for="inputValue" class="col-sm-3 control-label">Valor</label>
            <div class="col-sm-8">
                <input type="text" class="form-control" data-bind="textInput: Value" name="Number" id="inputValue" placeholder="Valor" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputDocumentType" class="col-sm-3 control-label">Tipo</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" data-bind="textInput: DocumentType().Name" name="Contact" id="inputDocumentType" placeholder="Tipo" />
            </div>
        </div>
    </form>
</div>