<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="product" content="Clinike">
    <meta name="description" content="Controle para consultórios médico, veterinário e odontológico">
    <meta name="author" content="Leandro Barral">

    <title>@ViewBag.Title - Clinike</title>

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,600,700,300&subset=latin" rel="stylesheet" type="text/css">

    <link href="~/Content/Css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Css/pixel-admin.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Css/widgets.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Css/rtl.css" rel="stylesheet" type="text/css">
    <link href="~/Content/Css/themes.css" rel="stylesheet" type="text/css">

    <!--[if lt IE 9]>
        <script src="~/Scripts/ie.min.js"></script>
    <![endif]-->
</head>
<body class="theme-default main-menu-animated">

    <script>var init = [];</script>

    <div id="main-wrapper">
        @Html.Partial("_Header")

        @Html.Partial("_Menu")
        
        <div id="content-wrapper">
            @Html.Partial("_Breadcrumb")

            @Html.Partial("_PageHeader")

            <div class="row">
                <div class="col-md-12">
                    @RenderBody()
                </div>
            </div>
            <div id="main-menu-bg"></div>
        </div>
    </div>

    <!--[if !IE]> -->
    <script type="text/javascript"> window.jQuery || document.write('<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js">' + "<" + "/script>"); </script>
    <!-- <![endif]-->
    <!--[if lte IE 9]>
        <script type="text/javascript"> window.jQuery || document.write('<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js">'+"<"+"/script>"); </script>
    <![endif]-->


    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/pixel-admin.js"></script>

    <script type="text/javascript">
        window.PixelAdmin.start(init);
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
